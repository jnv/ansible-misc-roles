export REDASH_CONNECTION_ADAPTER=pg
export REDASH_CONNECTION_STRING="dbname={{redash_pg_db}}"
export REDASH_STATIC_ASSETS_PATH="../rd_ui/dist/"
export REDASH_LOG_LEVEL="INFO"
export REDASH_WORKERS_COUNT={{redash_workers_count}}
export REDASH_REDIS_URL={{redash_redis_url}}
export REDASH_DATABASE_URL="postgresql://{{redash_pg_db}}"
export REDASH_COOKIE_SECRET={{redash_cookie_secret}}
export REDASH_GOOGLE_APPS_DOMAIN={{redash_google_apps_domain}}
{% if redash_enabled_query_runners is defined %}
export REDASH_ENABLED_QUERY_RUNNERS={{redash_enabled_query_runners | join(",")}}
{% endif %}
